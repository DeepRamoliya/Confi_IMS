@model IEnumerable<IMS.Model.RolesModel>@{ ViewBag.Title = "Index";    Layout = "~/Views/Shared/_Layout.cshtml";}            <div class="row">                <div class="col-4">                    <h2>Roles List</h2>                </div>                <div class="col-8">                    <div class="col-md-12 text-right">                        <a href="@Url.Action("CreateRole","Roles")" class="btn btn-outline-primary">Create New</a>                    </div>                </div>            </div>              <div class="table-responsive mt-3">                <table class="table ">                    <thead>                        <tr>                            <th>                                @Html.DisplayNameFor(model => model.First().Id)                            </th>                            <th>                                @Html.DisplayNameFor(model => model.First().Name)                            </th>                            <th>                                @Html.DisplayNameFor(model => model.First().RoleCode)                            </th>                            <th>                                @Html.DisplayNameFor(model => model.First().IsActive)                            </th>                            <th width="150" scope="col">Action</th>                        </tr>                    </thead>                    <tbody>                        @foreach (var item in Model)                        {                    <tr class="align-middle">                        <td>                            @Html.DisplayFor(modelItem => item.Id)                        </td>                        <td>                            @Html.DisplayFor(modelItem => item.Name)                        </td>                        <td>                            @Html.DisplayFor(modelItem => item.RoleCode)                        </td>                        <td>                            @if (@item.IsActive)                            {<div class="img-circle"><img src="~/Content/images/check.png" alt="editok" width="16"></div>}                            else                            { <div class=""><img src="~/Content/images/close.png" alt="edit" width="16"></div>}                        </td>                        <td>                            <div class="action-icons">                                <a href="@Url.Action("ViewPermission", "FormRoleMapping", new { Id = item.Id })"><img src="~/Content/images/Setting.png" style="margin-left: 4%" alt="edit" width="16"></a>                            </div>                        </td>                    </tr>                    <input type="hidden" id="hdnMenuCode" value="RoleMaster" />}                    </tbody>                    </table>            </div>