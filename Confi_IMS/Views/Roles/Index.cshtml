@model IEnumerable<IMS.Model.RolesModel>@{ ViewBag.Title = "Index";    Layout = "~/Views/Shared/_Layout.cshtml";}<!DOCTYPE html><html lang="en"><head></head><body>    <section class="section">        <div class="container">            <h4 class="h4 mb-4">Roles List</h4>            <div class="col-md-12 text-right">                    <a href="@Url.Action("CreateRole","Roles")" class="btn btn-primary">Add</a>                </div>            <div class="table-responsive">                <table class="table table-sm">                    <thead>                        <tr>                            <th>                                @Html.DisplayNameFor(model => model.First().Id)                            </th>                            <th>                                @Html.DisplayNameFor(model => model.First().Name)                            </th>                            <th>                                @Html.DisplayNameFor(model => model.First().RoleCode)                            </th>                            <th>                                @Html.DisplayNameFor(model => model.First().IsActive)                            </th>                            <th width="150" scope="col">Action</th>                        </tr>                    </thead>                    @foreach (var item in Model)                    {                <tbody>                    <tr class="align-middle">                        <td>                            @Html.DisplayFor(modelItem => item.Id)                        </td>                        <td>                            @Html.DisplayFor(modelItem => item.Name)                        </td>                        <td>                            @Html.DisplayFor(modelItem => item.RoleCode)                        </td>                        <td>                            @if (@item.IsActive)                            {<div class="img-circle"><img src="~/Content/images/check.png" alt="editok" width="16"></div>}                            else                            { <div class=""><img src="~/Content/images/close.png" alt="edit" width="16"></div>}                        </td>                        <td>                            <div class="action-icons">                                <a href="@Url.Action("ViewPermission", "FormRoleMapping", new { Id = item.Id })"><img src="~/Content/images/Setting.png" style="margin-left: 4%" alt="edit" width="16"></a>                                @*<a href="@Url.Action("Edit", "Roles", new { Id = item.Id })"><img src="~/Content/images/edit.png" style="margin-left: 4%" alt="edit" width="16"></a>*@                            </div>                        </td>                    </tr>                    <input type="hidden" id="hdnMenuCode" value="Roles" />                </tbody>}                </table>            </div>        </div>    </section></body></html>